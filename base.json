{
  "base": "morse",
  "lang": "hangul",
  "separators": {
    "between_characters": " ",
    "between_words": "  ",
    "compound_final_consonant_format": "{...}"
  },
  "consonants": {
    "ㄱ": "-", "ㄴ": "-*", "ㄷ": "*-*", "ㄹ": "-**", "ㅁ": "*-**",
    "ㅂ": "**-*", "ㅅ": "**-**", "ㅇ": "--", "ㅈ": "--*", "ㅊ": "*--*",
    "ㅋ": "--**", "ㅌ": "*--**", "ㅍ": "**--**", "ㅎ": "---",
    "ㄲ": "[-]", "ㄸ": "[*-*]", "ㅃ": "[**-*]", "ㅆ": "[**-**]"
  },
  "vowels": {
    "ㅣ": ":", "ㅏ": ":>", "ㅓ": "<:", "ㅑ": ";>", "ㅕ": "<;",
    "ㅐ": ";", "ㅒ": "~", "ㅔ": "<::", "ㅖ": "<;;", "ㅡ": "()",
    "ㅗ": "(^)", "ㅛ": "(^^)", "ㅜ": "(#)", "ㅠ": "(##)"
  },
  "compound_final_consonants": {
    "ㄳ": "{-}{**-**}",
    "ㄵ": "{-*}{--*}",
    "ㄶ": "{-*}{---}",
    "ㄺ": "{-**}{-}",
    "ㄻ": "{-**}{*-**}",
    "ㄼ": "{-**}{**-*}",
    "ㄽ": "{-**}{**-**}",
    "ㄾ": "{-**}{*--**}",
    "ㄿ": "{-**}{**--**}",
    "ㅀ": "{-**}{---}",
    "ㅄ": "{**-* }{**-**}"
  },
  "special_chars": {
    "!": "!!!",
    "?": "???",
    "@": ".__.",
    "#": "......",
    "$": "$$$",
    ".": "..."
  }
}
